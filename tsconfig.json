{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~/*": ["src/*"],
			"~components/*": ["src/components/*"],
			"~layouts/*": ["src/layouts/*"],
			"~pages/*": ["src/pages/*"],
			"~public/*": ["public/*"],
			"~styles/*": ["src/styles/*"],
			"~icons/*": ["src/icons/*"]
		}
	}
}
